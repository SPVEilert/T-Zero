{
  "name": "t-zero",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "backend": "npx ampx sandbox",
    "frontend": "npx expo start --tunnel",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky",
    "format-and-stage": "prettier --write .",
    "type-check": "tsc --noEmit",
    "update": "npx npm-check-updates -u && npm install",
    "check-updates": "npx npm-check-updates",
    "check-updates:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "aws-amplify": "^6.15.4",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "react": "19.0.0",
    "react-native": "0.79.5"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "aws-cdk-lib": "^2.204.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true
}
